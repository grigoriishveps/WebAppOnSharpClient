@model IEnumerable<Note>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>AllNotes</h2>
<div class="container">
    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Patient</th>
            <th scope="col">Doctor</th>
            <th scope="col">Disease</th>
            <th scope="col">Data</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var forecast in this.Model)
        {
            <tr>
                <th scope="row">@forecast.Id</th>
                <td>@forecast.PatientId</td>
                <td>@forecast.DoctorId</td>
                <td>@forecast.DiseaseId</td>
                <td>@forecast.DateVisit.ToString()</td>
            </tr>
        }
        </tbody>
    </table>
</div>