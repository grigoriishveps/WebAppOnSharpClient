
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Add Doctor</h2>
<form method="post" action="/Doctor/Put">
    <div class="form-group">
        <label for="Initials">Initials</label>
        <input type="text" name="Initials" class="form-control" id="Initials" aria-describedby="emailHelp">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="Room">Password</label>
        <input type="text" name="Room" class="form-control" id="Room">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>